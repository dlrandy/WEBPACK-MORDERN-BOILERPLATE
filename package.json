{
  "name": "webpack-poilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --config ./webpack.config.dev.js --display-error-details",
    "unit:test": "jest --coverage",
    "start:dev": "nodemon --watch webpack.config.dev.js --exec \"webpack-dev-server --config webpack.config.dev.js\"",
    "start-dev": "webpack-dev-server --config webpack.config.dev.js",
    "start-serve": "json-server --delay 1000 --watch ./server/db.json --routes ./server/routes.json",
    "start": "node ./startUp.js",
    "end2end": "",
    "lint": "eslint . --cache --cache-location node_modules/.cache/eslint",
    "format": "pretty-quick",
    "doc": "esdoc",
    "start-build": "webpack --config ./webpack.config.prod.js src/index.js src/index.html",
    "build": "npm-run-all -p build:*",
    "build:poll": "webpack --mode production --display-optimization-bailout"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.2.3",
    "axios": "^0.18.0",
